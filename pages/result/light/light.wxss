/* pages/result/light/light.wxss */
.container-result {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.title{
  position: absolute;
  left: 270rpx;
  top:10vh;
  width: 40%;
  text-align: center;
  font-size: 5.8vw;
  font-weight: bolder;
  background-color: #f4eebe;
  border: 30rpx solid #f4eebe;
  border-radius: 20rpx;
}

.text{
  position: relative;
  top: 10vh;
  width: 80vw;
  font-weight: bolder;
}

.back-button{
  position: relative;
  top: 13vh;
  text-align: center;
  font-size: 5.8vw;
  font-weight: bolder;
  background-color: #f3c1c4;
  border: 30rpx solid #f3c1c4;
  border-radius: 20rpx;
}
.help-button{
  position: relative;
  top: 17vh;
  left: 25vw;
  text-align: center;
  font-size: 4.5vw;
  font-weight: bolder;
  background-color: #ebeaf0;
  border: 30rpx solid #ebeaf0;
  border-radius: 20rpx;
  box-shadow: 10px 5px 5px #dee0ec;
}